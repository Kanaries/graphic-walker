+  });
+});
+
+describe('on the horizontal axis', () => {
+  const preset = getPreset(horizontal);
+  const state = getStatePreset(horizontal);
+
+  it('should move forward on a MOVE_RIGHT', () => {
+    const result: ?PublicResult = moveInDirection({
