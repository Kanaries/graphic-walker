+    descriptor: {
+      id: 'inForeign4',
+      droppableId: foreign.descriptor.id,
+      type: foreign.descriptor.type,
+      index: 3,
+    },
+    borderBox: {
+      [axis.start]: 100,
+      [axis.crossAxisStart]: foreignCrossAxisStart,
+      [axis.crossAxisEnd]: foreignCrossAxisEnd,
+      [axis.end]: 140,
