+
+  return {
+    home,
+    inHome1,
+    inHome2,
+    inHome3,
+    inHome4,
+    foreign,
+    inForeign1,
+    inForeign2,
+    inForeign3,
+    inForeign4,
+    emptyForeign,
+    droppables,
+    draggables,
+    dimensions,
+    inHomeList,
+    inForeignList,
+    windowScroll,
+    viewport,
+  };
+};
