+  title: 'Done',
+  taskIds: [],
+};
+
+const entities: Entities = {
+  columnOrder: [todo.id, done.id],
+  columns: {
+    [todo.id]: todo,
+    [done.id]: done,
+  },
+  tasks: taskMap,
+};
+
+export default entities;
