+                dimension: preset.inHome3,
+                shouldAnimate: false,
+              },
+              {
+                dimension: preset.inHome4,
+                shouldAnimate: false,
+              },
+            ],
+          }),
+          displacedBy,
+          at: {
+            type: 'REORDER',
+            destination: {
