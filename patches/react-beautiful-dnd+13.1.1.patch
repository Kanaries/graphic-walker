+    // we are faking a lift from a scrolled window
+    const result: Viewport = {
+      frame: base.frame,
+      scroll: {
+        initial: windowScroll,
+        current: windowScroll,
+        max: base.scroll.max,
+        diff: {
+          value: origin,
+          displacement: origin,
+        },
+      },
