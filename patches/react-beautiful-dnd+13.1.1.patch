+
+  function lift(quoteId: string): ?SnapDragActions {
+    if (isDragging) {
+      return null;
+    }
+
+    const api: ?SensorAPI = sensorAPIRef.current;
+
+    if (!api) {
+      console.warn('unable to find sensor api');
+      return null;
+    }
+
+    const preDrag: ?PreDragActions = api.tryGetLock(quoteId, noop);
