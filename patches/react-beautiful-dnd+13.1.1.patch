+      index: 0,
+    },
+    borderBox: {
+      // starting inside the droppable border box
+      // account for own margin
+      [axis.start]:
+        droppableBorderBoxStart + assortedSpacing.margin[axis.crossAxisStart],
+      [axis.crossAxisStart]:
