+  expect(warn.mock.calls[0][0]).toEqual(
+    // dimensions will be ordered by index
+    // inHome1: index 0: all good
+    // inHome3: index 2: boom
+    // inHome4: index 3: all good (2 + 1)
+    // inHome2: index 6: boom
+    expect.stringContaining(`0, [üî•2], 3, [üî•6]`),
+  );
+
+  warn.mockRestore();
+});
