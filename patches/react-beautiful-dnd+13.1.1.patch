+  expect(responders.onDragStart).toHaveBeenCalledTimes(1);
+  expect(responders.onDragUpdate).toHaveBeenCalledTimes(2);
+  expect(responders.onDragEnd).toHaveBeenCalledWith(result, expect.any(Object));
