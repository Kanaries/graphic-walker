+it('should block a click after a drag', () => {
+  const { getByText } = render(<App />);
+  const handle: HTMLElement = getByText('item: 0');
+
+  simpleLift(touch, handle);
+  act(() => touch.drop(handle));
+
+  const click: Event = createEvent.click(handle);
