+
+        const expected: MapProps = {
+          mapped: {
+            type: 'SECONDARY',
+            shouldAnimateDisplacement: true,
+            offset: displacedBy.point,
+            combineTargetFor: null,
+            snapshot: getSecondarySnapshot({
