+    mocks.scrollWindow.mockClear();
+
+    // Now passing event more time (60%)
+    mockNow.mockReturnValueOnce(startAcceleratingAt + accelerationRange * 0.6);
+    scroller.scroll(
+      dragTo({
+        selection: onMaxBoundary,
+        viewport: scrollableViewport,
+        state,
