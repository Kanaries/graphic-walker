+            onClick={() =>
+              maybe((callbacks: SnapDragActions) => callbacks.moveDown())
+            }
+            disabled={!isDragging}
+            label="down"
+          >
+            ‚Üì
+          </ArrowButton>
+          <ArrowButton type="button" disabled={!isDragging} label="right">
+            ‚Üí
+          </ArrowButton>
+        </div>
+      </ArrowBox>
