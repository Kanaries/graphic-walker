+// 120 second timeout
+jest.setTimeout(120 * 1000);
+
+async function clean() {
+  await exec('yarn build:clean');
+}
+
+beforeAll(async () => {
+  await clean();
+  await exec('yarn build');
+});
+
+afterAll(clean);
+
+it('should contain warnings in development', async () => {
+  const filePath: string = './dist/react-beautiful-dnd.js';
