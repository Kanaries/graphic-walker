+  const [isControlDragging, setIsControlDragging] = useState(false);
+  const sensorAPIRef = useRef<?SensorAPI>(null);
+
+  const onDragEnd = useCallback(
+    function onDragEnd(result: DropResult) {
+      setIsDragging(false);
