+      const expected: DragImpact = {
+        ...current,
+        at: {
+          type: 'COMBINE',
+          combine: {
+            draggableId: preset.inForeign2.descriptor.id,
+            droppableId: preset.foreign.descriptor.id,
+          },
+        },
+      };
+      expect(result).toEqual(expected);
+    });
