+      goAfter: inHome1,
+      droppable: home,
+      borderBoxSize: 20,
+    }),
+    ...assortedSpacing,
+    windowScroll,
+  });
+  // size: 30
+  const inHome3: DraggableDimension = getDraggableDimension({
+    descriptor: {
+      id: 'inhome3',
+      droppableId: home.descriptor.id,
