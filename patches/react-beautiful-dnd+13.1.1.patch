+  render() {
+    return (
+      <DragDropContext onDragEnd={this.onDragEnd}>
+        <Root>
+          <AuthorList
+            listId="AUTHOR"
+            internalScroll={this.props.internalScroll}
+            isCombineEnabled={this.props.isCombineEnabled}
+            quotes={this.state.quotes}
+          />
+        </Root>
+      </DragDropContext>
+    );
+  }
+}
