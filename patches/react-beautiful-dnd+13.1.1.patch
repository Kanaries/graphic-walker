+  store.dispatch(moveUp());
+  expect(responders.onDragStart).not.toHaveBeenCalled();
+  expect(responders.onDragUpdate).not.toHaveBeenCalled();
+
+  store.dispatch(
+    completeDrop({
+      completed: getCompletedWithResult(result, store.getState()),
+    }),
