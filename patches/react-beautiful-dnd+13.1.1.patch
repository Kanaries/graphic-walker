+  act(() => touch.cancel(handle));
+
+  const click: Event = createEvent.click(handle);
+  fireEvent(handle, click);
+
+  expect(click.defaultPrevented).toBe(false);
+  expect(onDragStart).not.toHaveBeenCalled();
+});
