+    mocks.scrollWindow.mockClear();
+
+    // Moving 30% of the way into the time dampening period
+    mockNow.mockReturnValueOnce(startAcceleratingAt + accelerationRange * 0.3);
+    scroller.scroll(
+      dragTo({
+        selection: onMaxBoundary,
+        viewport: scrollableViewport,
