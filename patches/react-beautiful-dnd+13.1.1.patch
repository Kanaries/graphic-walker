+  expect(unregisterSpy).not.toHaveBeenCalled();
+  registerSpy.mockClear();
+
+  forceUpdate(wrapper);
+  expect(unregisterSpy).not.toHaveBeenCalled();
+  expect(registerSpy).not.toHaveBeenCalled();
+});
