{
  "name": "@kanaries/graphic-walker",
  "version": "0.4.0-a",
  "scripts": {
    "dev:front_end": "vite --host",
    "dev": "npm run dev:front_end",
    "build": "tsc && vite build",
    "build:app": "vite build --config vite-app.config.ts",
    "serve": "vite preview",
    "type": "tsc src/lib.ts --declaration --emitDeclarationOnly --jsx react --esModuleInterop --outDir dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "Apache-2.0",
  "main": "./dist/graphic-walker.umd.js",
  "module": "./dist/graphic-walker.es.js",
  "exports": {
    ".": {
      "import": "./dist/graphic-walker.es.js",
      "require": "./dist/graphic-walker.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 160,
    "singleQuote": true
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "^2.0.8",
    "@kanaries/react-beautiful-dnd": "0.0.2",
    "@kanaries/web-data-loader": "^0.1.7",
    "@tailwindcss/forms": "^0.5.4",
    "autoprefixer": "^10.3.5",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "immer": "^9.0.15",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "nanoid": "^4.0.2",
    "postcss": "^8.3.7",
    "postinstall-postinstall": "^2.1.0",
    "re-resizable": "^6.9.8",
    "react-i18next": "^11.18.6",
    "react-shadow": "^20.0.0",
    "rxjs": "^7.3.0",
    "tailwindcss": "^3.2.4",
    "uuid": "^8.3.2",
    "vega": "^5.22.1",
    "vega-embed": "^6.21.0",
    "vega-lite": "^5.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/react": "^17.x",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.x",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.1",
    "@vercel/analytics": "^0.1.8",
    "@vitejs/plugin-react": "^3.1.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.3.2",
    "vite": "^4.1.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0 <19.0.0",
    "react-dom": ">=17.0.0 <19.0.0",
    "styled-components": "^5.3.6"
  }
}
